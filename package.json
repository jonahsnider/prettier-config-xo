{
	"name": "prettier-config-xo",
	"version": "0.0.0-development",
	"keywords": [
		"xo",
		"prettier",
		"prettier-config",
		"xo-config"
	],
	"bugs": {
		"url": "https://github.com/jonahsnider/prettier-config-xo/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jonahsnider/prettier-config-xo.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Jonah Snider",
		"email": "jonah@jonah.pw",
		"url": "https://jonah.pw"
	},
	"exports": {
		".": "./tsc_output/index.js",
		"./space": "./tsc_output/space.js",
		"./space.js": "./tsc_output/space.js"
	},
	"main": "./tsc_output/index.js",
	"scripts": {
		"prebuild": "rm -rf tsc_output",
		"build": "tsc",
		"deploy": "semantic-release",
		"lint": "xo",
		"style": "prettier --ignore-path .gitignore --check .",
		"pretest": "rm -rf coverage .nyc_output",
		"test": "nyc ava"
	},
	"prettier": "@jonahsnider/prettier-config",
	"ava": {
		"extensions": [
			"ts"
		],
		"files": [
			"!tsc_output"
		],
		"require": [
			"ts-node/register",
			"source-map-support/register"
		]
	},
	"nyc": {
		"all": true,
		"extends": "@istanbuljs/nyc-config-typescript",
		"include": [
			"src/**/*.ts"
		],
		"reporter": [
			"lcov",
			"cobertura"
		]
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "1.0.2",
		"@jonahsnider/prettier-config": "1.1.2",
		"@jonahsnider/xo-config": "5.2.0",
		"@semantic-release/exec": "6.0.3",
		"@types/prettier": "2.7.0",
		"ava": "4.3.3",
		"eslint-plugin-prettier": "4.2.1",
		"nyc": "15.1.0",
		"prettier": "2.7.1",
		"semantic-release": "19.0.5",
		"source-map-support": "0.5.21",
		"ts-node": "10.9.1",
		"typescript": "4.8.2",
		"xo": "0.52.3"
	},
	"engines": {
		"node": ">=10.13.0"
	}
}
