{
	"name": "prettier-config-xo",
	"version": "0.0.0-development",
	"keywords": [
		"xo",
		"prettier",
		"prettier-config",
		"xo-config"
	],
	"bugs": {
		"url": "https://github.com/jonahsnider/prettier-config-xo/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jonahsnider/prettier-config-xo.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Jonah Snider",
		"email": "jonah@jonah.pw",
		"url": "https://jonah.pw"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./space": {
			"types": "./dist/space.d.ts",
			"import": "./dist/space.mjs",
			"require": "./dist/space.js"
		}
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "zshy",
		"deploy": "semantic-release",
		"lint": "xo",
		"lint:fix": "xo --fix",
		"style": "prettier --check .",
		"pretest": "rm -rf coverage .nyc_output",
		"test": "c8 ava"
	},
	"prettier": "@jonahsnider/prettier-config",
	"devDependencies": {
		"@jonahsnider/prettier-config": "1.1.2",
		"@semantic-release/exec": "6.0.3",
		"@types/prettier": "3.0.0",
		"ava": "6.4.1",
		"c8": "10.1.3",
		"eslint-plugin-prettier": "5.5.4",
		"prettier": "3.6.2",
		"semantic-release": "22.0.12",
		"tsx": "4.20.6",
		"typescript": "5.9.3",
		"xo": "0.61.1",
		"zshy": "0.4.4"
	},
	"packageManager": "yarn@3.8.7",
	"engines": {
		"node": ">=10.13.0"
	},
	"zshy": {
		"exports": {
			".": "./src/index.ts",
			"./space": "./src/space.ts"
		}
	}
}
