{
	"name": "prettier-config-xo",
	"version": "0.0.0-development",
	"keywords": [
		"xo",
		"prettier",
		"prettier-config",
		"xo-config"
	],
	"bugs": {
		"url": "https://github.com/jonahsnider/prettier-config-xo/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jonahsnider/prettier-config-xo.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Jonah Snider",
		"email": "jonah@jonah.pw",
		"url": "https://jonah.pw"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./space": {
			"types": "./dist/space.d.ts",
			"import": "./dist/space.mjs",
			"require": "./dist/space.js"
		}
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "zshy",
		"deploy": "semantic-release",
		"lint": "xo --prettier",
		"lint:fix": "xo --fix",
		"style": "prettier --check .",
		"pretest": "rm -rf coverage .nyc_output",
		"test": "nyc ava"
	},
	"prettier": "@jonahsnider/prettier-config",
	"ava": {
		"extensions": [
			"ts"
		],
		"files": [
			"!dist"
		],
		"require": [
			"ts-node/register",
			"source-map-support/register"
		]
	},
	"nyc": {
		"all": true,
		"extends": "@istanbuljs/nyc-config-typescript",
		"include": [
			"src/**/*.ts"
		],
		"reporter": [
			"lcov",
			"cobertura"
		]
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "1.0.2",
		"@jonahsnider/prettier-config": "1.1.2",
		"@jonahsnider/xo-config": "5.3.0",
		"@semantic-release/exec": "6.0.3",
		"@types/prettier": "3.0.0",
		"ava": "5.3.1",
		"eslint-plugin-prettier": "5.5.4",
		"nyc": "15.1.0",
		"prettier": "3.6.2",
		"semantic-release": "22.0.12",
		"source-map-support": "0.5.21",
		"ts-node": "10.9.2",
		"typescript": "5.9.3",
		"xo": "0.61.1",
		"zshy": "0.4.4"
	},
	"packageManager": "yarn@3.8.1",
	"engines": {
		"node": ">=10.13.0"
	},
	"zshy": {
		"exports": {
			".": "./src/index.ts",
			"./space": "./src/space.ts"
		}
	}
}
